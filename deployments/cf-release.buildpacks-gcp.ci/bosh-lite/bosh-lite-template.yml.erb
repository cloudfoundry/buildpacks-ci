  - name: nats
    release: bosh
  - name: blobstore
    release: bosh
  - name: postgres
    release: bosh
  - name: director
    release: bosh
  - name: health_monitor
    release: bosh
  - name: warden_cpi
    release: bosh-warden-cpi
  - name: garden
    release: garden-linux
  - name: port_forwarding
    release: port-forwarding
