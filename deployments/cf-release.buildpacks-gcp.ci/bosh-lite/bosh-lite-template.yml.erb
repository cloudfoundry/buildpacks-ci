      rules:
      - internal_port: 80
        external_port: 80
        internal_ip: 10.244.0.34
      - internal_port: 443
        external_port: 443
        internal_ip: 10.244.0.34
      - internal_port: 2222
        external_port: 2222
        internal_ip: 10.244.0.34
      - internal_port: 4443
        external_port: 4443
        internal_ip: 10.244.0.34
