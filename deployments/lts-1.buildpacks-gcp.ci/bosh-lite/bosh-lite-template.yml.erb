  version: latest
update:
  canaries: 1
  max_in_flight: 1
  canary_watch_time: 5000 - 600000
  update_watch_time: 5000 - 600000
jobs:
- name: bosh
  instances: 1
  azs:
  - z1
  vm_type: bosh-lite-worker
  stemcell: trusty
  persistent_disk_pool: bosh-ssd
  networks:
  - name: public
    default:
    - dns
    - gateway
  - name: vip
    static_ips:
    - 104.196.204.208
  templates:
  - name: nats
    release: bosh
  - name: blobstore
    release: bosh
  - name: postgres
    release: bosh
  - name: director
    release: bosh
  - name: health_monitor
    release: bosh
  - name: warden_cpi
    release: bosh-warden-cpi
  - name: garden
    release: garden-linux
  - name: port_forwarding
    release: port-forwarding
  properties:
    port_forwarding:
      rules:
      - internal_port: 80
        external_port: 80
        internal_ip: 10.244.0.34
      - internal_port: 443
        external_port: 443
        internal_ip: 10.244.0.34
      - internal_port: 2222
        external_port: 2222
        internal_ip: 10.244.0.34
      - internal_port: 4443
        external_port: 4443
        internal_ip: 10.244.0.34
